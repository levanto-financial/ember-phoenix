{
  "name": "ember-phoenix",
  "version": "0.1.7",
  "description": "Elxir/Phoenix awesomeness for Ember.js apps",
  "keywords": [
    "ember-addon",
    "phoenix",
    "elixir"
  ],
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "directories": {
